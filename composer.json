{
    "name": "torq/torq-cms",
    "description": "The Torq CMS",
    "type": "project",
    "keywords": [
        "framework",
        "cms"
    ],
    "license": "Apache-2.0",
    "version": "3.0.0",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "doctrine/orm": "^2.6.2",
        "phpseclib/phpseclib": "~2.0",
        "symfony/dependency-injection": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/finder": "^4.4",
        "symfony/http-foundation": "4.4.20",
        "symfony/http-kernel": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "roave/security-advisories": "dev-master"
    },
    "archive" : {
        "exclude" : [
            ".*"
        ]
    },
    "autoload" : {
      "psr-4": {
          "Torq\\": "engine/"
      },

      "classmap": [
          "engine/*"
      ]
    },
    "config" : {
      "platform": {
        "php": "7.2"
      },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
